{
  "name": "unnamed-ui",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:ci": "ng serve --port 4200 --disable-host-check",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage --watch=false --browsers=FirefoxHeadless",
    "test:ci": "ng test --code-coverage --watch=false --browsers=FirefoxHeadless",
    "lint": "ng lint",
    "storybook": "ng run ngx-unnamed:storybook",
    "build-storybook": "ng run ngx-unnamed:build-storybook",
    "docs:generate": "compodoc -p tsconfig.compodoc.json -d documentation --theme material --disablePrivate --disableInternal --disableSourceCode",
    "docs:serve": "compodoc -p tsconfig.json -s -d documentation",
    "docs:build": "npm run build && npm run docs:generate && npm run build-storybook",
    "lhci": "lhci autorun",
    "lhci:upload": "lhci upload",
    "chromatic": "chromatic --project-token=your-project-token-here",
    "bundle-size": "node scripts/bundle-size-check.js",
    "build:parallel": "node scripts/build-parallel.js",
    "build:profile": "node scripts/build-profile.js",
    "watch:test": "node scripts/test-watch-performance.js",
    "tree-shaking:analyze": "node scripts/tree-shaking-analyzer.js",
    "tree-shaking:test": "node scripts/tree-shaking-test.js",
    "metrics:collect": "node scripts/build-metrics-collector.js npm run build:parallel",
    "metrics:report": "node -e \"const c = require('./scripts/build-metrics-collector'); new c().generatePerformanceReport();\"",
    "prepare": "husky"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.7",
    "@angular/cdk": "^20.2.10",
    "@angular/common": "^20.1.7",
    "@angular/compiler": "^20.1.7",
    "@angular/core": "^20.1.7",
    "@angular/forms": "^20.1.7",
    "@angular/platform-browser": "^20.1.7",
    "@angular/platform-browser-dynamic": "^20.1.7",
    "@angular/router": "^20.1.7",
    "rxjs": "~7.8.0",
    "svg-to-ts": "^12.0.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.1.7",
    "@angular/build": "^20.1.6",
    "@angular/cli": "^20.1.6",
    "@angular/compiler-cli": "^20.1.7",
    "@compodoc/compodoc": "^1.1.31",
    "@lhci/cli": "^0.15.1",
    "@storybook/addon-a11y": "^8.6.14",
    "@storybook/addon-docs": "^8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/angular": "^8.6.14",
    "@types/jasmine": "~5.1.0",
    "angular-eslint": "20.1.1",
    "axe-core": "^4.11.0",
    "chromatic": "^13.3.0",
    "eslint": "^9.28.0",
    "eslint-plugin-storybook": "^0.11.2",
    "husky": "^9.1.7",
    "jasmine-axe": "^1.1.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-firefox-launcher": "^2.1.3",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ng-packagr": "^20.1.0",
    "storybook": "^8.6.14",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.33.1"
  }
}
